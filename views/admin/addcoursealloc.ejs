<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>Untitled Document <%= title %></title>
</head>

<body>
<form id="form1" name="form1" method="post" action="/addcourseallocation">
  <table width="200" border="1">
    <tr>
      <td>Course </td>
      <td><label>
        <select name="select" id="select">
          <option>Select</option>
          <% cnn.forEach(i=>{ %>
            <option value="<%= i._id%>"><%= i.course_name %></option>

       <% }); %>
        </select>
      </label></td>
    </tr>
    <tr>
      <td>Intern </td>
      <td><label>
        <select name="select2" id="select">
          <option>Select</option>
          <% mnn.forEach(i=>{ %>
            <option value="<%= i._id%>"><%= i.Name %></option>

       <% }); %>
        </select>
      </label></td>
    </tr>
    <tr>
      <td><input type="submit" name="button" id="button" value="Add" /></td>
    </tr>
  </table>
</form>
</body>
</html>
