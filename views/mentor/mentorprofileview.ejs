<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <title>Mentor Profile</title>
  <style>
    body {
      font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
      background-color: #f4f4f4;
      margin: 0;
      display: flex;
      align-items: center;
      justify-content: center;
      height: 100vh;
    }

    form {
      margin-top: 20px;
    }

    table {
      width: 400px;
      border-collapse: collapse;
      background-color: #fff;
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    }

    td {
      padding: 12px;
      border: 1px solid #ddd;
      text-align: left;
    }

    img {
      display: block;
      margin: 0 auto;
      max-width: 100%;
      height: auto;
    }
  </style>
</head>

<body>
  <form action="/mentorpro" method="get">
    <table>
      <tr>
        <td colspan="2" style="text-align: center;"><img name="Photo" src="<%= data.photo %>" alt="Mentor Photo" /></td>
      </tr>
      <tr>
        <td>Name</td>
        <td><%= data.Name %></td>
      </tr>
      <tr>
        <td>Phone.no</td>
        <td><%= data.Phone %></td>
      </tr>
      <tr>
        <td>Email</td>
        <td><%= data.Dob %></td>
      </tr>
      <tr>
        <td>State</td>
        <td><%= data.State %></td>
      </tr> 
      <tr>
        <td>City</td>
        <td><%= data.City %></td>
      </tr> 
      <tr>
        <td>PIN</td>
        <td><%= data.PIN %></td>
      </tr>
      <tr>
        <td>Qualification</td>
        <td><%= data.Phone %></td>
      </tr>
      <tr>
        <td>Email</td>
        <td><%= data.Email %></td>
      </tr>
      <tr>
        <td>Qualifications</td>
        <td><%= data.Qualifications %></td>
      </tr>
      <tr>
        <td>Join Date</td>
        <td><%= data.join_date %></td>
      </tr>
    </table>
  </form>
</body>
</html>
